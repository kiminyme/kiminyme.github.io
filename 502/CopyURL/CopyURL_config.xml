      <rdf:Description tsc:version="2.0.1" dc:date="2013-07-06 05:05:42 PM" dc:source="Camtasia Studio,8.0.4,enu" dc:title="CopyURL" tscDM:firstFrame="CopyURL_First_Frame.png" tscDM:originId="75e7c712-75de-4261-88e5-cd92103791b5" tscDM:project="CopyURL">
         <xmpDM:duration xmpDM:scale="1/1000" xmpDM:value="78466"/>
